{% form_theme form theme %}
</br>
<style>
#form_login label {
  font-weight: bold;
  }
</style>
<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register" id="form_login">
 <br/>
   <div><span>{{ form_label(form.username) }}</span> {{ form_errors(form.username) }}{{ form_widget(form.username) }}</div>
   <div><span>{{ form_label(form.email) }}</span> {{ form_errors(form.email) }}{{ form_widget(form.email) }}</div>
   
{{ form_rest(form) }}
   <!--<div><span>{{ form_label(form.plainPassword.first) }}</span> {{ form_errors(form.plainPassword.first) }}{{ form_widget(form.plainPassword.first) }}</div>
    <div><span>{{ form_label(form.plainPassword.second) }}</span> {{ form_errors(form.plainPassword.second) }}{{ form_widget(form.plainPassword.second) }}</div>--->
   <div>
        <input type="submit" value="{{ 'registration.submit'|trans({}, 'FOSUserBundle') }}" class="btn"/>
    </div>
   <a href="{{ path('fos_user_security_logout')}}" style="margin-left:300px">{{ 'layout.login'|trans({}, 'FOSUserBundle') }} </a>
           
</form>
